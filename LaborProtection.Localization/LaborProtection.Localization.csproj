<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="en_EN.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>en_EN.resx</DependentUpon>
    </Compile>
    <Compile Update="ru_RU.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ru_RU.resx</DependentUpon>
    </Compile>
    <Compile Update="ua_UA.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ua_UA.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="en_EN.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>en_EN.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="ru_RU.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>ru_RU.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="ua_UA.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>ua_UA.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
